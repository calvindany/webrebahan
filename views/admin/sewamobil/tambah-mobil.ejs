    <%- include('../include/header.ejs') %>
        <link rel="stylesheet" href="/css/adminRoot.css">
    </head>
        <%- include('../include/navigation.ejs') %>

        <section class="row custom-padding">
            <h3 class="my-4">Tambah Paket Perjalanan</h3>
        </section>

        <form class="custom-padding" method="POST" enctype="multipart/form-data" action="/admin/tambahsewamobil">
            <div class="mb-3">
                <label for="namamobil" class="form-label">Nama Mobil</label>
                <input type="text" class="form-control" name="namamobil" aria-describedby="emailHelp">
            </div>
            <div class="mb-3">
                <label for="harga" class="form-label">Harga</label>
                <input type="number" class="form-control" name="harga">
            </div>
            <div class="mb-3">
                <label for="Durasi Wisata" class="form-label">Kapasitas</label>
                <div class="d-flex justify-content-start align-items-center col-3">
                    <input type="number" class="form-control" name="kapasitas" placeholder="Kapasitas Mobil">
                    <label class="mx-3" for="kapasitas">Orang</label>
                </div>
            </div>
            <div class="mb-3">
                <label for="driver" class="form-label">Driver</label>
                <div class="d-flex justify-content-start align-items-center col-3">
                    <label class="mx-3" for="driver">Rp. </label>
                    <input type="number" class="form-control" name="driver" placeholder="Harga Driver">
                </div>
            </div>
            <div class="mb-3">
                <label for="images" class="form-label">Upload Images</label>
                <input type="file" class="form-control" name="images">
            </div>
            <button type="submit" class="btn btn-primary">Submit</button>
        </form>


        <%- include('../include/footer.ejs') %>